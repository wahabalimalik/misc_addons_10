  <template id="theme_customize" inherit_id="theme_bootswatch.theme_customize" name="theme_customize Bootswatch">
    <xpath expr="//label[hasclass('checked')]" position="replace">
          <label>
            <img style="width: 120px; height: 70px;" src="/website_custom/static/src/img/sections/slider/1.jpg" alt="Custom"/>
            <input name="theme" type="radio" data-xmlid="website_custom.theme_custom"/>
          </label>
    </xpath>
  </template>
