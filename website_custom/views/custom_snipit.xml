<odoo>
<template id="custom_snippets" inherit_id="website.snippets">
  <xpath expr="//div[@id='snippet_structure']" position="before">
    <div id="snippet_custom_structure" class="o_panel">
      <div class="o_panel_header">
          <i class="fa fa-heartbeat"/>Custom Structures
      </div>
      <div class="o_panel_body">
        <t t-snippet="website_custom.s_home" 
          t-thumbnail="/website/static/src/img/blocks/block_banner.png"
          />
        <t t-snippet="website_custom.s_about_us" 
          t-thumbnail="/website/static/src/img/blocks/block_title.png"
          />
        <t t-snippet="website_custom.s_who_we_are" 
          t-thumbnail="/website/static/src/img/blocks/block_text_image.png"
          />
        
